{
  "name": "dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --open",
    "build": "rm -rf build/_app/immutable && vite build && touch build/.gitkeep",
    "build:default": "vite build",
    "preview": "vite preview",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "perf:dev": "cd perf/dev && lighthouse http://localhost:5173/ --output html --output json --view && cd ../..",
    "perf": "cd perf/preview && lighthouse http://localhost:4173/ --output html --output json --view && cd ../.."
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@poppanator/sveltekit-svg": "^4.2.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "browserslist-to-esbuild": "^2.1.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-svelte": "^2.35.1",
    "lighthouse": "^11.4.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.12",
    "vitest": "^1.2.1"
  },
  "type": "module",
  "dependencies": {
    "@zerodevx/svelte-toast": "^0.9.5",
    "echarts": "^5.4.3",
    "i18next": "^23.7.6",
    "polished": "^4.2.2",
    "svelte-i18next": "^2.2.2",
    "tippy.js": "^6.3.7"
  }
}
